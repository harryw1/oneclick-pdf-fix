{
  "functions": {
    "src/pages/api/**/*.ts": {
      "maxDuration": 120
    },
    "src/pages/api/process.ts": {
      "maxDuration": 120
    },
    "src/pages/api/upload.ts": {
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "crons": [
    {
      "path": "/api/process-queue",
      "schedule": "*/5 * * * *"
    }
  ]
}