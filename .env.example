# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_public_key
SUPABASE_API_KEY=your_supabase_secret_key

# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# App Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret

# Rate Limiting (Optional - Redis)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# Google Cloud Configuration (Required for enhanced processing)
# Project ID and bucket name are automatically extracted from the service account JSON
GOOGLE_CREDENTIALS_BASE64=your_base64_encoded_service_account_json

# Vercel Blob Storage Configuration
BLOB_READ_WRITE_TOKEN=your_vercel_blob_read_write_token

# Cron Authentication
CRON_SECRET=your_cron_secret_for_vercel_functions

# Processing Configuration
MAX_CONCURRENT_OPERATIONS=5
VISION_API_TIMEOUT_MS=600000